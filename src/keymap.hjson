
# キーマップで使用する言語
lang: jp

# キーマップで使用するキーコードの短縮形定義
define: {
	LSFT: KeyLeftShift
	LCTL: KeyLeftCtrl
	LALT: KeyLeftAlt
}

# キーマップ
